{
  "name": "timer",
  "version": "0.3.0",
  "scripts": {
    "prepare": "husky",
    "test": "mocha --config .mocharc.json",
    "lint": "eslint --ext js,ts,tsx src/ webpack/",
    "prettier-check": "prettier \"**/*.ts{,x}\" --check",
    "pre-commit": "npm run test && npm run lint && npm run prettier-check",
    "pre-push": "npm run build; npm run deploy",
    "dev": "webpack serve --config ./webpack/config.dev.ts",
    "build": "rm -rf build; webpack --progress --config ./webpack/config.prod.ts",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.5",
    "@types/ramda": "^0.29.12",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "chai": "^4.3.7",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fit-commit-js": "^0.3.2",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "mini-css-extract-plugin": "^2.8.1",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "moment": "^2.30.1",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  }
}
