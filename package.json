{
  "name": "timer",
  "version": "0.3.0",
  "scripts": {
    "prepare": "husky",
    "test": "mocha --config .mocharc.json",
    "lint": "eslint",
    "prettier-check": "prettier \"**/*.ts{,x}\" --check",
    "pre-commit": "npm run test && npm run lint && npm run prettier-check",
    "pre-push": "npm run build; npm run deploy",
    "dev": "webpack serve --config ./webpack/config.dev.ts",
    "build": "rm -rf build; npx tsc --noEmit;  webpack --progress --config ./webpack/config.prod.ts",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.5",
    "@types/ramda": "^0.29.12",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/uuid": "^9.0.8",
    "chai": "^4.3.7",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fit-commit-js": "^0.3.2",
    "gh-pages": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "mini-css-extract-plugin": "^2.9.2",
    "mocha": "^10.4.0",
    "prettier": "^3.4.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "moment": "^2.30.1",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  }
}
