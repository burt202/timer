{
  "name": "timer",
  "version": "0.3.0",
  "scripts": {
    "prepare": "husky install",
    "test": "mocha --config .mocharc.json",
    "lint": "eslint --ext js,ts,tsx src/ webpack/",
    "prettier": "prettier \"**/*.ts{,x}\" --check",
    "pre-commit": "npm run test && npm run lint && npm run prettier",
    "pre-push": "npm run build; npm run deploy",
    "dev": "webpack serve --config ./webpack/config.dev.ts",
    "build": "rm -rf build; webpack --progress --config ./webpack/config.prod.ts",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.9",
    "@types/ramda": "^0.28.20",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "chai": "^4.3.7",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "fit-commit-js": "^0.3.2",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "mini-css-extract-plugin": "^2.7.2",
    "mocha": "^10.2.0",
    "prettier": "^2.8.1",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  }
}
