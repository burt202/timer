{
  "name": "timer",
  "version": "0.3.0",
  "scripts": {
    "prepare": "husky install",
    "test": "mocha --config .mocharc.json",
    "lint": "eslint --ext js,ts,tsx src/ webpack/",
    "prettier": "prettier \"**/*.ts{,x}\" --check",
    "pre-commit": "npm run test && npm run lint && npm run prettier",
    "pre-push": "npm run build; npm run deploy",
    "dev": "webpack serve --config ./webpack/config.dev.ts",
    "build": "rm -rf build; webpack --progress --config ./webpack/config.prod.ts",
    "deploy": "gh-pages -d build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/fit-commit-js/lib/hook.js .git/COMMIT_EDITMSG",
      "pre-commit": "npm run lint",
      "pre-push": "npm run prepush"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.33",
    "@types/ramda": "^0.27.62",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "chai": "^4.3.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.25.3",
    "fit-commit-js": "^0.3.2",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "mini-css-extract-plugin": "^2.7.2",
    "mocha": "^9.1.3",
    "prettier": "^2.8.1",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "moment": "^2.29.1",
    "ramda": "^0.22.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
