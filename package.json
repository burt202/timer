{
  "name": "timer",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint --ext js,ts,tsx src/",
    "test": "mocha --config .mocharc.json",
    "start": "webpack serve --progress --config ./webpack/config.dev.js",
    "build": "rm -rf build; webpack --progress --config ./webpack/config.prod.js",
    "prepush": "npm run build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/fit-commit-js/lib/hook.js .git/COMMIT_EDITMSG",
      "pre-commit": "npm run lint",
      "pre-push": "npm run prepush"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.33",
    "@types/ramda": "^0.27.62",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "chai": "^4.3.4",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^8.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "fit-commit-js": "^0.3.2",
    "husky": "^4.3.8",
    "mini-css-extract-plugin": "^2.2.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "nunjucks": "^3.2.3",
    "nunjucks-webpack-plugin": "git@github.com:burt202/nunjucks-webpack-plugin.git",
    "prettier": "^2.5.1",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "moment": "^2.29.1",
    "ramda": "^0.22.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
